1. (& operator)

#include <stdio.h>

int main()
{
	int x=27;
	
	printf("Value of x: %d\n",x);
	printf("Address of x: %X\n",&x);
	
	return 0;
}

2. (Arithmetic opeartors)

#include <stdio.h>
int main()
{
    int a = 9,b = 4, c;
    
    c = a+b;
    printf("a+b = %d \n",c);
    c = a-b;
    printf("a-b = %d \n",c);
    c = a*b;
    printf("a*b = %d \n",c);
    c = a/b;
    printf("a/b = %d \n",c);
    c = a%b;
    printf("Remainder when a divided by b = %d \n",c);
    
    return 0;
}

3. (++ operator and -- operator)

#include <stdio.h>
int main()
{
    int a = 10, b = 100;
    float c = 10.5, d = 100.5;

    printf("++a = %d \n", ++a);
    printf("--b = %d \n", --b);
    printf("++c = %f \n", ++c);
    printf("--d = %f \n", --d);

    return 0;
}

4. (= operator)

#include <stdio.h>
int main()
{
    int a = 5, c;

    c = a;
    printf("c = %d\n", c);

    return 0;
}

5. ( == operator)

#include <stdio.h>
int main()
{
   int m=40,n=20;
   if (m == n)
   {
       printf("m and n are equal");
   }
   else
   {
       printf("m and n are not equal");
   }
}

6. (Relational operators)

#include <stdio.h>
int main()
{
    int a = 5, b = 5;
    
    printf("%d > %d is %d \n", a, b, a > b);
    printf("%d < %d is %d \n", a, b, a < b);
    printf("%d != %d is %d \n", a, b, a != b);
    printf("%d >= %d is %d \n", a, b, a >= b);
    printf("%d <= %d is %d \n", a, b, a <= b);

    return 0;
}



